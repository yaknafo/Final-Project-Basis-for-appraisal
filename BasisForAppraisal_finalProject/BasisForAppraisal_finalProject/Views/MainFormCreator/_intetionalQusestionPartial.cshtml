@model BasisForAppraisal_finalProject.DBML.tbl_IntentionalQuestion
@{
    ViewBag.Title = "tbl_IntentionalQuestion";
}
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<h4 style="font-weight:bold">@Model.Title</h4>
<table class="table" dir="rtl">
    <th>
        @for (int i = 0; i < Model.Answers.Count(); i++)
        {
            @Html.TextAreaFor(x => x.Answers[i].Text, new { style = "height: 200px;  width:200px;margin-left:10%; ", disabled = "disabled" })
        }
        <br />
        @for (int i = 0; i < Model.Answers.Count(); i++)
        {
            @Html.RadioButtonFor(a => a.selectedAnswer, Model.Answers[i].AnswerId, new { style = "height: 25px;  width:25px; margin-right: 6%;margin-left: 20%; margin-top: 10px;"})
        }
    </th>





</table>


