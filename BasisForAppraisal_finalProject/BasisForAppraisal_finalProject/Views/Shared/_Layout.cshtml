<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>בסיס להערכה</title>
    @*@Styles.Render("~/Content/css")*@
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.1.3.min.js"></script>
    @Scripts.Render("~/bundles/modernizr")
    <script>
        $(document).ready(function () {
            $("#lay").mouseover(function () {
                $("#lay").css("background-color", "#666666");
            });
            $("#lay").mouseout(function () {
                $("#lay").css("background-color", "inherit");
            });
        });

        $(document).ready(function () {
            $("#proj").mouseover(function () {
                $("#proj").css("background-color", "#666666");
            });
            $("#proj").mouseout(function () {
                $("#proj").css("background-color", "inherit");
            });
        });
    </script>


</head>
<body >
    <div class="navbar navbar-inverse navbar-fixed-top" dir="rtl" style="background-color:#F6F9F9">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*<img  id ="img_logo" alt="Logo" src="~/photos/logosmall.jpg" />*@
                @*@Html.ActionLink("בסיס להערכה", "Index", "Home", null, new { @class = "navbar-brand" })*@
               
            </div>
            <div class="navbar-collapse collapse" dir="rtl" style="font-family:My Custom Font">
                <ul class="nav navbar-nav" style="margin-top: 1%; font-size:22px;font-weight: bold;">
                  @*<li>@Html.Partial("_Back")</li>*@

                    <li>@Html.ActionLink("ניהול הרשאות", "", "")</li>
                    <li>@Html.ActionLink("ניתוח מידע", "", "")</li>
                    <li id="lay">@Html.ActionLink("ניהול טפסים", "backToMainForm", "MainFormCreator")</li>
                    <li id="proj">@Html.ActionLink("ניהול לקוחות", "MainCompanies", "MainCompanies")</li>

                    @*
                    <li>@Html.ActionLink("שליחת טפסים ללקוח", "About", "Home")</li>
                    <li>@Html.ActionLink("הוספת לקוח", "Contact", "Home")</li>*@
                </ul>
                @*@Html.Partial("_LoginPartial")*@
                <a href="/Home/Index">   <img id="img_logo" alt="Logo" src="~/photos/Logo/Icon_Basis_for_evaluation_Tran.gif"style="margin-left:30%; height:60px; width:200px;" /></a>
            </div>
        </div>
    </div>
    <div class="container body-content" >
        @RenderBody()
        <hr style="margin-top:50%;" />

    </div>

    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
