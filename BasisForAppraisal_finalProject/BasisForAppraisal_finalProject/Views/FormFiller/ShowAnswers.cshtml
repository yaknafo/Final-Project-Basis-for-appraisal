@model BasisForAppraisal_finalProject.ViewModel.FormFillerViewModel
@using PagedList;
@using PagedList.Mvc;
@{
    Layout = null;
}
<!DOCTYPE html>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery-2.1.3.min.js"></script>
<script src="~/Scripts/jquery.simplePagination.min.js"></script>
<link href="~/Content/style.css" rel="stylesheet" />
<script src="~/Scripts/OurJavaScripts/FormFillJS.js"></script>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>FormFill</title>
    <script type="text/javascript">
        $(function () {
            var perPage = 3;
            var opened = 1;
            var onClass = 'on';
            var paginationSelector = '.pages';
            $('#exampsle').simplePagination(perPage, opened, onClass, paginationSelector);
        });
    </script>


    <meta name="viewport" content="width=device-width" />
    <title>ShowAnswers</title>
</head>
<body>
    <div class="alert-info">
        <h1 style="text-align: center">@Model.FillOn על  @Model.FillBy</h1>
    </div>
    <div id="Panel" style="width: 100%; margin-top: 0%; background-color:white ">
        <div class="panel panel-heading">
            <div class="panel-heading">
                <div class="alert-info">
                    @Html.DisplayFor(x => x.From)
                </div>
                <div id="exampsle">
                    @Html.DisplayFor(x => x.Questions)
                </div>
                <ul class="pager">
                    <li class="pages"></li>
                </ul>
            </div>
        </div>
    </div>
</body>
</html>
