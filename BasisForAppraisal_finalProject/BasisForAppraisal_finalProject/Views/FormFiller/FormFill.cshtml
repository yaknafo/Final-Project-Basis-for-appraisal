@model BasisForAppraisal_finalProject.ViewModel.FormFillerViewModel
 @using PagedList;
@using PagedList.Mvc;
@{
    Layout = null;
}

<!DOCTYPE html>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery-2.1.3.min.js"></script>
<script src="~/Scripts/jquery.simplePagination.min.js"></script>
<link href="~/Content/style.css" rel="stylesheet" />
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>FormFill</title>
    <script type="text/javascript">
        $(function () {
            var perPage = 3;
            var opened = 1;
            var onClass = 'on';
            var paginationSelector = '.pages';
            $('#exampsle').simplePagination(perPage, opened, onClass, paginationSelector);
        });
    </script>
   

</head>
<body>


    @using(Html.BeginForm())
    { 
        @Html.HiddenFor(x => x.CompanyId)
        @Html.HiddenFor(x => x.FillBy)
        @Html.HiddenFor(x => x.FillOn)
         @Html.HiddenFor(x => x.FormIdConnector)
    <div id="Panel" style="width: 100%; margin-top: 5%; background-color:white ">
        <div class="panel panel-heading">
            <div class="panel-heading">
                @Html.EditorFor(x => x.From)
                <div id="exampsle">
                    @Html.EditorFor(x => x.Questions)
                    <button type="submit" class="btn btn-primary btn-lg" style="width:150px; height:10%">סיים ושלח</button>
               </div>
                <ul class="pager">
                    <li class="pages"></li>
                   
                    </ul>

               </div>
           </div>
        </div>
    
    }
</body>
</html>
